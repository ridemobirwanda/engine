{
  "name": "engine-frontend",
  "type": "javascript",
  "main": "functions/[[path]].js",
  "compatibility_date": "2024-01-01",
  "build": {
    "command": "npm run build",
    "cwd": "./",
    "watch_paths": ["src/**/*.tsx", "src/**/*.ts", "public/**/*"]
  },
  "env": {
    "production": {
      "name": "engine-frontend-prod"
    }
  }
}

